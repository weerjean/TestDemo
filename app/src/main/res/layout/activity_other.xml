            android:layout_weight="1"
            android:text="进程列表1"/>

        <Button
            android:id="@+id/btn_other_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="进程列表2"/>

        <Button
            android:id="@+id/btn_other_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="进程列表3"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/btn_other_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="hook"/>

        <Button
            android:id="@+id/btn_other_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="功能十"/>

        <Button
            android:id="@+id/btn_other_11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="功能11"/>

        <Button
            android:id="@+id/btn_other_12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="功能12"/>

    </LinearLayout>

    <TextView
        android:id="@+id/tv_other_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test"/>

    <ImageView
        android:id="@+id/iv_other_screenshot_show"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
